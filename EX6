using System;

class Program
{
    static void Main()
    {
        double sum = 0;

        while (true)
        {
            Console.Write("Nhap mot so: ");
            double number = Convert.ToDouble(Console.ReadLine());

            if (number == 0)
                break;

            if (number < 0)
             continue;
                sum += number;
        }

        Console.WriteLine($"Tong cac so duong da nhap: {sum}");
    }
}
